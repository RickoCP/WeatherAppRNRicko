{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
      "baseUrl": ".",
      "paths": {
          "@data/*": [
              "./src/data/*"
          ],
          "@domains/*": [
              "./src/domain/*"
          ],
          "@presentation/*": [
              "./src/presentation/*"
          ],
          "@core/*": [
              "./src/core/*"
          ]
      }
  },
  "typeRoots": ["./types"]
}
